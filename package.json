      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-plugin-module-resolver": "^4.1.0"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "src"
          ],
          "cwd": "packagejson"
        }
      ]
    ]
  }
