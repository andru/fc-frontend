  "private": true,
  "type": "module",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "can-ndjson-stream": "^1.0.2",
    "dotenv": "^8.2.0",
    "greenlet": "^1.1.0",
    "grommet-icons": "^4.4.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-list": "^0.8.13",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.0.1",
    "wikibase-sdk": "^7.4.1"
